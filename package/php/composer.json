{
  "name": "aether-vault/php-sdk",
  "description": "Secure Aether Vault SDK for PHP applications",
  "type": "library",
  "keywords": [
    "security",
    "secrets",
    "vault",
    "aether",
    "enterprise"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "nyholm/psr7": "^1.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "AetherVault\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AetherVault\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
